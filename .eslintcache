epageLayout>\n        )} />\n\n        <Route exact path=\"/search\" render={() => (\n          <MainLayout>\n            <Search />\n          </MainLayout>\n        )}></Route>\n\n        <Route path=\"/search/:filterType\" render={() => (\n          <MainLayout>\n            <Search />\n          </MainLayout>\n        )}></Route>\n\n        <Route path=\"/product/:productID\" render={() => (\n          <MainLayout>\n            <ProductDetails />\n          </MainLayout>\n        )}></Route>\n\n        <Route path=\"/cart\" render={() => (\n          <WithAuth>\n            <MainLayout>\n              <Cart />\n            </MainLayout>\n          </WithAuth>\n        )} />\n\n        <Route path=\"/payment\" render={() => (\n          <WithAuth>\n            <MainLayout>\n              <Payment />\n            </MainLayout>\n          </WithAuth>\n        )} />\n\n        <Route path='/registration'\n          render={() => (\n            <MainLayout >\n              <Registration />\n            </MainLayout>\n          )} />\n\n        <Route path='/login'\n          render={() => (\n            <MainLayout>\n              <Login />\n            </MainLayout>\n          )} />\n\n        <Route path='/recovery' render={() => (\n          <MainLayout>\n            <Recovery />\n          </MainLayout>\n        )} />\n\n        <Route path='/dashboard' render={() => (\n          <WithAuth>\n            <DashBoardLayout>\n              <Dashboard />\n            </DashBoardLayout>\n          </WithAuth>\n        )} />\n\n        <Route path='/orders/:orderID' render={() => (\n          <WithAuth>\n            <DashBoardLayout>\n              <Order />\n            </DashBoardLayout>\n          </WithAuth>\n        )} />\n\n        <Route path='/admin' render={() => (\n          <WithAdminAuth>\n            <AdminLayout>\n              <Admin />\n            </AdminLayout>\n          </WithAdminAuth>\n        )} />\n      </Switch>\n    </div>\n  );\n}\n\nexport d