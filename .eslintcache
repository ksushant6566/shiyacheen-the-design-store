patch();\n    const { currentUser, orderHistory } = useSelector(mapState);\n\n    useEffect(() => {\n        dispatch(\n            getUserOrderHistory(currentUser.id)\n        )\n    }, [])\n\n\n    return (\n        <div>\n            <h1>\n                Order History\n            </h1>\n\n            <Orderhistory orders={orderHistory} />\n        </div>\n    )\n}\n\nexport default Dashboard;","/Users/ksushant6566/Desktop/personal-projects/shiyacheen-the-design-store/src/pages/Payment/index.js",[],"/Users/ksushant6566/Desktop/personal-projects/shiyacheen-the-design-store/src/pages/Cart/index.js",[],"/Users/ksushant6566/Desktop/personal-projects/shiyacheen-the-design-store/src/pages/Recovery/index.js",[],"/Users/ksushant6566/Desktop/personal-projects/shiyacheen-the-design-store/src/pages/Order/index.js",["319"],"import React, { useEffect } from 'react';\nimport { useParams } from \"react-router-dom\";\nimport { useDispatch, useSelector } from 'react-redux';\n\nimport { getOrderDetailsStart } from '../../redux/Orders/orders.actions';\nimport OrderDeatils from \"../../components/OrderDetails\";\n\nconst mapState = ({ orders }) => ({\n    orderDetails: orders.orderDetails\n});\n\nconst Order = () => {\n    const dispatch = useDispatch();\n    const { orderID } = useParams();\n    const { orderDetails } = useSelector(mapState);\n    const { orderTotal } = orderDetails;\n\n    useEffect(() => {\n        dispatch(getOrderDetailsStart(orderID))\n    }, []);\n\n    return (\n        <div>\n            <h1>\n                Order ID: {orderID}\n            </h1>\n\n            <OrderDeatils order={orderDetails} />\n\n            <h4>\n                Total : ₹{orderTotal}\n            </h4>\n        </div>\n    )\n};\n\nexport default Order;","/Users/ksushant6566/Desktop/personal-projects/shiyacheen-the-design-store/src/pages/Login/index.js",[],"/Users/ksushant6566/Desktop/personal-projects/shiyacheen-the-design-store/src/pages/Homepage/index.js",[],"/Users/ksushant6566/